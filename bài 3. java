gói  mồi bản ;
nhập  java . sử dụng . Máy quét ;
 lớp  công khai bai3 {
    public  static  void  main ( String [] args ) {
        Máy quét  sc = Máy quét mới  ( Hệ thống . In );

        Hệ thống . ra ngoài . println ( "Mời bạn nhập vào tên:" );
         Tên chuỗi = sc . nextLine ();
        Hệ thống . ra ngoài . println ( "Mời bạn nhập vào tu:" );
        int  age = sc . nextInt ();
        nếu ( tuổi < 16 ) {
            Hệ thống . ra ngoài . println ( "Bạn" + tên + "ởđộtổivịthànhniên" );
        } else  if ( age > = 16 && age < 18 ) {
            Hệ thống . ra ngoài . println ( "Bạn" + tên + "ởđộtuổitrưởngthành" );
        }
        khác  nếu ( tuổi > = 16 && tuổi < 18 ) {
            Hệ thống . ra ngoài . println ( "Bạn" + tên + "đãgià" );
        }
