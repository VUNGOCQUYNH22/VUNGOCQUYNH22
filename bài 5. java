nhập  java . sử dụng . Máy quét ;
 lớp  công khai bai5 {
    public  static  void  main ( String [] args ) {
        Máy quét  sc = Máy quét mới  ( Hệ thống . In );
       int  tong ;
        làm {
            Hệ thống . ra ngoài . print ( "nhap vao n:" );
            int  n = sc . nextInt ();
            Hệ thống . ra ngoài . print ( "nhap vao m:" );
            int  m = sc . nextInt ();
            tong = m + n ;
            if ( tong < 100 ) { Hệ thống . ra ngoài . println ( "tong nay <100" );}
           else  if ( tong > = 100 ) { Hệ thống . ra ngoài . println ( "tong nay> = 100" );}

        } while ( tong < 100 );
}}
